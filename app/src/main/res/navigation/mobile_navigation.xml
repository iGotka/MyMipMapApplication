    <fragment
        android:id="@+id/navigation_reg"
        android:name="com.example.mymipmapapplication.activities.bottom.ui.dashboard.MenuFragment"
        android:label="@string/title_reg"
        tools:layout="@layout/fragment_menu" />

    <fragment
        android:id="@+id/navigation_auth"
        android:name="com.example.mymipmapapplication.activities.bottom.ui.authorization.AuthFragment"
        android:label="@string/title_auth"
